{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"type": "object",
	"javaImplements": [
		"SerializedDataBase"
	],
	"properties": {
		"sidingId": {
			"type": "integer"
		},
		"previousRouteId": {
			"type": "integer"
		},
		"previousPlatformId": {
			"type": "integer"
		},
		"previousStationId": {
			"type": "integer"
		},
		"previousRouteColor": {
			"type": "integer"
		},
		"previousRouteName": {
			"type": "string"
		},
		"previousStationName": {
			"type": "string"
		},
		"previousRouteDestination": {
			"type": "string"
		},
		"thisRouteId": {
			"type": "integer"
		},
		"thisPlatformId": {
			"type": "integer"
		},
		"thisStationId": {
			"type": "integer"
		},
		"thisRouteColor": {
			"type": "integer"
		},
		"thisRouteName": {
			"type": "string"
		},
		"thisStationName": {
			"type": "string"
		},
		"thisRouteDestination": {
			"type": "string"
		},
		"nextPlatformId": {
			"type": "integer"
		},
		"nextStationId": {
			"type": "integer"
		},
		"nextStationName": {
			"type": "string"
		},
		"isTerminating": {
			"type": "boolean"
		},
		"interchangeColorsForStationNameList": {
			"type": "array",
			"items": {
				"$ref": "interchangeColorsForStationName.json"
			}
		},
		"stoppingPoint": {
			"type": "number"
		},
		"doorTarget": {
			"type": "boolean"
		},
		"railLength": {
			"type": "number",
			"minimum": 0
		},
		"totalVehicleLength": {
			"type": "number",
			"minimum": 0
		},
		"repeatIndex1": {
			"type": "integer",
			"minimum": 0
		},
		"repeatIndex2": {
			"type": "integer",
			"minimum": 0
		},
		"acceleration": {
			"type": "number",
			"minimum": 0
		},
		"isManualAllowed": {
			"type": "boolean"
		},
		"maxManualSpeed": {
			"type": "number",
			"minimum": 0
		},
		"manualToAutomaticTime": {
			"type": "integer",
			"minimum": 0
		},
		"totalDistance": {
			"type": "number",
			"minimum": 0
		},
		"defaultPosition": {
			"type": "number",
			"minimum": 0
		},
		"vehicleCars": {
			"type": "array",
			"items": {
				"$ref": "vehicleCar.json"
			}
		},
		"path": {
			"type": "array",
			"items": {
				"$ref": "pathData.json"
			}
		},
		"ridingEntities": {
			"type": "array",
			"items": {
				"$ref": "vehicleRidingEntity.json"
			}
		},
		"brakingPower": {
			"type": "number",
			"minimum": 0
		}
	},
	"required": [
		"sidingId",
		"railLength",
		"totalVehicleLength",
		"repeatIndex1",
		"repeatIndex2",
		"acceleration",
		"isManualAllowed",
		"maxManualSpeed",
		"manualToAutomaticTime",
		"totalDistance",
		"defaultPosition"
	]
}
