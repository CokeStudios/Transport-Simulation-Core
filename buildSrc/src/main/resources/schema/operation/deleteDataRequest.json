		"stationIds": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"platformIds": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"sidingIds": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"routeIds": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"depotIds": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"liftFloorPositions": {
			"type": "array",
			"items": {
				"$ref": "position.json"
			}
		},
		"railIds": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"railNodePositions": {
			"type": "array",
			"items": {
				"$ref": "position.json"
			}
		}
