{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"type": "object",
	"javaImplements": [
		"SerializedDataBase"
	],
	"properties": {
		"platformIds": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"count": {
			"type": "integer",
			"minimum": 1
		},
		"page": {
			"type": "integer",
			"minimum": 0
		},
		"realtimeOnly": {
			"type": "boolean"
		}
	},
	"required": [
		"count",
		"page",
		"realtimeOnly"
	]
}
