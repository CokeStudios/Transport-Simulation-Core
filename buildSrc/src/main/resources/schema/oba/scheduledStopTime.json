		"SerializedDataBase"
	],
	"properties": {
		"arrivalEnabled": {
			"type": "boolean"
		},
		"arrivalTime": {
			"type": "integer",
			"minimum": 0
		},
		"departureEnabled": {
			"type": "boolean"
		},
