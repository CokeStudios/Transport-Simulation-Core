{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"type": "object",
	"javaImplements": [
		"SerializedDataBase"
	],
	"properties": {
		"stations": {
			"type": "array",
			"items": {
				"$ref": "station.json"
			}
		},
		"platforms": {
			"type": "array",
			"items": {
				"$ref": "platform.json"
			}
		},
		"sidings": {
			"type": "array",
			"items": {
				"$ref": "siding.json"
			}
		},
		"routes": {
			"type": "array",
			"items": {
				"$ref": "route.json"
			}
		},
		"depots": {
			"type": "array",
			"items": {
				"$ref": "depot.json"
			}
		},
		"simplifiedRoutes": {
			"type": "array",
			"items": {
				"$ref": "simplifiedRoute.json"
			}
		},
		"rails": {
			"type": "array",
			"items": {
				"$ref": "rail.json"
			}
		},
		"positions": {
			"type": "array",
			"items": {
				"$ref": "position.json"
			}
		},
		"signals": {
			"type": "array",
			"items": {
				"$ref": "signalModification.json"
			}
		},
		"vehiclesToUpdate": {
			"type": "array",
			"items": {
				"$ref": "vehicleUpdate.json"
			}
		},
		"vehiclesToKeep": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"vehiclesToRemove": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"liftsToUpdate": {
			"type": "array",
			"items": {
				"$ref": "lift.json"
			}
		},
		"liftsToKeep": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"liftsToRemove": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		}
	}
}
